{
  "options": {
    "experimentalClearLocaleDirs": true,
    "jsonSchema": {
      "./docs.json": {
        "composite": {
          "$.navigation.languages": {
            "type": "array",
            "key": "$.language",
            "experimentalSort": "localesAlphabetical",
            "include": [
              "$..group",
              "$..tab",
              "$..item",
              "$..anchor",
              "$..dropdown"
            ],
            "transform": {
              "$..pages[*]": {
                "match": "^(.*)$",
                "replace": "{locale}/$1"
              },
              "$..root": {
                "match": "^(.*)$",
                "replace": "{locale}/$1"
              }
            }
          }
        }
      }
    },
    "docsUrlPattern": "/[locale]",
    "docsImportPattern": "/snippets/[locale]",
    "generateRedirects": "./docs.json",
    "experimentalLocalizeStaticImports": true,
    "experimentalLocalizeStaticUrls": true,
    "docsHideDefaultLocaleImport": true,
    "experimentalHideDefaultLocale": true,
    "experimentalAddHeaderAnchorIds": "mintlify",
    "mintlify": {
      "openapi": {
        "files": [
          "./openapi/analyst-reports-raw-text-api_api.spec.yml",
          "./openapi/calendar_api.spec.yml",
          "./openapi/data-api-proxy_api.spec.yml",
          "./openapi/delivery_api.spec.yml",
          "./openapi/earnings-call-transcripts-api_api.spec.yml",
          "./openapi/logo-api_api.spec.yml",
          "./openapi/news-api_api.spec.yml",
          "./openapi/newsquantified-api_api.spec.yml",
          "./openapi/ticker-trends-api_api.spec.yml",
          "./openapi/webhook_api.spec.yml"
        ]
      }
    },
    "yamlSchema": {
      "./openapi/analyst-reports-raw-text-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/calendar_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/data-api-proxy_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/earnings-call-transcripts-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/logo-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/news-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/newsquantified-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/ticker-trends-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/webhook_api.spec.yml": {
        "preset": "openapi"
      },
      "./openapi/delivery_api.spec.yml": {
        "preset": "openapi"
      }
    }
  },
  "locales": [
    "en",
    "es",
    "ja",
    "ko",
    "ar"
  ],
  "defaultLocale": "en",
  "$schema": "https://assets.gtx.dev/config-schema.json",
  "files": {
    "json": {
      "include": [
        "./docs.json"
      ]
    },
    "mdx": {
      "include": [
        "./**/*.mdx"
      ],
      "transform": {
        "match": "^(snippets/)?(.*)$",
        "replace": "$1{locale}/$2"
      },
      "exclude": [
        "./[locales]/**/*.mdx",
        "./snippets/[locales]/**/*.mdx"
      ]
    },
    "yaml": {
      "include": [
        "./openapi/analyst-reports-raw-text-api_api.spec.yml",
        "./openapi/calendar_api.spec.yml",
        "./openapi/data-api-proxy_api.spec.yml",
        "./openapi/delivery_api.spec.yml",
        "./openapi/earnings-call-transcripts-api_api.spec.yml",
        "./openapi/logo-api_api.spec.yml",
        "./openapi/news-api_api.spec.yml",
        "./openapi/newsquantified-api_api.spec.yml",
        "./openapi/ticker-trends-api_api.spec.yml",
        "./openapi/webhook_api.spec.yml"
      ],
      "transform": [
        {
          "match": "^openapi/analyst-reports-raw-text-api_api.spec.yml$",
          "replace": "{locale}/openapi/analyst-reports-raw-text-api_api.spec.yml"
        },
        {
          "match": "^openapi/calendar_api.spec.yml$",
          "replace": "{locale}/openapi/calendar_api.spec.yml"
        },
        {
          "match": "^openapi/data-api-proxy_api.spec.yml$",
          "replace": "{locale}/openapi/data-api-proxy_api.spec.yml"
        },
        {
          "match": "^openapi/delivery_api.spec.yml$",
          "replace": "{locale}/openapi/delivery_api.spec.yml"
        },
        {
          "match": "^openapi/earnings-call-transcripts-api_api.spec.yml$",
          "replace": "{locale}/openapi/earnings-call-transcripts-api_api.spec.yml"
        },
        {
          "match": "^openapi/logo-api_api.spec.yml$",
          "replace": "{locale}/openapi/logo-api_api.spec.yml"
        },
        {
          "match": "^openapi/news-api_api.spec.yml$",
          "replace": "{locale}/openapi/news-api_api.spec.yml"
        },
        {
          "match": "^openapi/newsquantified-api_api.spec.yml$",
          "replace": "{locale}/openapi/newsquantified-api_api.spec.yml"
        },
        {
          "match": "^openapi/ticker-trends-api_api.spec.yml$",
          "replace": "{locale}/openapi/ticker-trends-api_api.spec.yml"
        },
        {
          "match": "^openapi/webhook_api.spec.yml$",
          "replace": "{locale}/openapi/webhook_api.spec.yml"
        }
      ]
    }
  }
}