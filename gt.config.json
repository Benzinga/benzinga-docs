{
  "options": {
    "experimentalClearLocaleDirs": true,
    "jsonSchema": {
      "./docs.json": {
        "composite": {
          "$.navigation.languages": {
            "type": "array",
            "key": "$.language",
            "experimentalSort": "localesAlphabetical",
            "include": [
              "$..group",
              "$..tab",
              "$..item",
              "$..anchor",
              "$..dropdown"
            ],
            "transform": {
              "$..pages[*]": {
                "match": "^(.*)$",
                "replace": "{locale}/$1"
              },
              "$..root": {
                "match": "^(.*)$",
                "replace": "{locale}/$1"
              }
            }
          }
        }
      }
    },
    "docsUrlPattern": "/[locale]",
    "docsImportPattern": "/snippets/[locale]",
    "generateRedirects": "./docs.json",
    "experimentalLocalizeStaticImports": true,
    "experimentalLocalizeStaticUrls": true,
    "docsHideDefaultLocaleImport": true,
    "experimentalHideDefaultLocale": true,
    "experimentalAddHeaderAnchorIds": "mintlify",
    "mintlify": {
      "openapi": {
        "files": [
          "./analyst-reports-raw-text-api_api.spec.yml",
          "./calendar_api.spec.yml",
          "./data-api-proxy_api.spec.yml",
          "./earnings-call-transcripts-api_api.spec.yml",
          "./logo-api_api.spec.yml",
          "./news-api_api.spec.yml",
          "./newsquantified-api_api.spec.yml",
          "./ticker-trends-api_api.spec.yml"
        ]
      }
    },
    "yamlSchema": {
      "./analyst-reports-raw-text-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./calendar_api.spec.yml": {
        "preset": "openapi"
      },
      "./data-api-proxy_api.spec.yml": {
        "preset": "openapi"
      },
      "./earnings-call-transcripts-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./logo-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./news-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./newsquantified-api_api.spec.yml": {
        "preset": "openapi"
      },
      "./ticker-trends-api_api.spec.yml": {
        "preset": "openapi"
      }
    }
  },
  "locales": [
    "en",
    "es",
    "ja"
  ],
  "defaultLocale": "en",
  "$schema": "https://assets.gtx.dev/config-schema.json",
  "files": {
    "json": {
      "include": [
        "./docs.json"
      ]
    },
    "mdx": {
      "include": [
        "./**/*.mdx"
      ],
      "transform": {
        "match": "^(snippets/)?(.*)$",
        "replace": "$1{locale}/$2"
      },
      "exclude": [
        "./[locales]/**/*.mdx",
        "./snippets/[locales]/**/*.mdx"
      ]
    },
    "yaml": {
      "include": [
        "./analyst-reports-raw-text-api_api.spec.yml",
        "./calendar_api.spec.yml",
        "./data-api-proxy_api.spec.yml",
        "./earnings-call-transcripts-api_api.spec.yml",
        "./logo-api_api.spec.yml",
        "./news-api_api.spec.yml",
        "./newsquantified-api_api.spec.yml",
        "./ticker-trends-api_api.spec.yml"
      ],
      "transform": [
        {
          "match": "^analyst-reports-raw-text-api_api.spec.yml$",
          "replace": "{locale}/analyst-reports-raw-text-api_api.spec.yml"
        },
        {
          "match": "^calendar_api.spec.yml$",
          "replace": "{locale}/calendar_api.spec.yml"
        },
        {
          "match": "^data-api-proxy_api.spec.yml$",
          "replace": "{locale}/data-api-proxy_api.spec.yml"
        },
        {
          "match": "^earnings-call-transcripts-api_api.spec.yml$",
          "replace": "{locale}/earnings-call-transcripts-api_api.spec.yml"
        },
        {
          "match": "^logo-api_api.spec.yml$",
          "replace": "{locale}/logo-api_api.spec.yml"
        },
        {
          "match": "^news-api_api.spec.yml$",
          "replace": "{locale}/news-api_api.spec.yml"
        },
        {
          "match": "^newsquantified-api_api.spec.yml$",
          "replace": "{locale}/newsquantified-api_api.spec.yml"
        },
        {
          "match": "^ticker-trends-api_api.spec.yml$",
          "replace": "{locale}/ticker-trends-api_api.spec.yml"
        }
      ]
    }
  }
}